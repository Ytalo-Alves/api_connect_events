{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "22.0.0",
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": [
      "es2024",
      "ESNext.Array",
      "ESNext.Collection",
      "ESNext.Iterator"
    ],
    "module": "nodenext",
    "moduleResolution": "node16",
    "skipLibCheck": true,
    "strict": true,
    "target": "es2022"
  }
}